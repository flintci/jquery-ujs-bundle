{
    "name": "flintci/jquery-ujs-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle adapter for jQuery-ujs and CSRF protection",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/event-dispatcher": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/security-csrf": "^3.4 || ^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "FlintCI\\jQueryUJSBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlintCI\\jQueryUJSBundle\\Tests\\": "tests"
        }
    }
}
